<aside>
   <img src="{{user.imgPath}}" alt="" style="width:100px;height:100px">
   <h1>{{user.username}}</h1>
   <h3><a href="/social/profile">My Profile</a></h3>
   <h3><a href="/social/friends">Friends</a></h3>
   <h3><a href="/social/settings">Settings</a></h3>
</aside>
<style>
   #map {
      height: 500px;
      width: 100%;
   }
</style>
<h3>My Trips:</h3>
<div class="trips">
   {{#each user.trips}}
      <div class="trip">
         {{!-- <img src="the used image on trips" alt=""> --}}
         <h3>{{this.title}}</h3>
         <a href=""><button>Chat</button></a>
      </div>
   {{/each}}
</div>
<div id="map"></div>

<script src="https://maps.googleapis.com/maps/api/js?key={{myEnv.GOOGLE_MAPS_API_KEY}}"></script>
<script src="/javascripts/map.js"></script>
