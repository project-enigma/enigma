<div class="signup">
  <div class="signup-form">

    <img class="logo-login-signup" src="../images/enigma-logo-white.svg" alt="">

    <form action="/auth/signup" method="POST" id="form-container">

   <div class="imput-box">
      <input class="imput" id="email" type="email" name="email" placeholder="email">
      <br><br>
      <input class="imput" id="username" type="text" name="username" placeholder="username">
      <br><br>
      <input class="imput" id="password" type="password" name="password" placeholder="password">
   </div>


      <a href="/auth/google">Sign In with Google</a>
      <br>
      {{#if message }}
      <div class="error-message">{{ message }}</div>
      {{/if}}
      <br><br>

      <button class="btn-login-signup">Create account</button>

      
      </p>
    </form>
    
  </div>
  <p class="account-message">
        Do you already have an account?
        <a href="/auth/login">Login</a>
</div>


